{
  "name": "iterator-cascade-callbacks",
  "version": "0.0.6",
  "description": "Iterator helper methods that enable chaining",
  "main": "iterator-cascade-callbacks.js",
  "scripts": {
    "ts-build:ci-cd": "tsc --build",
    "ts-build:es2021": "tsc --project tsconfig.es2021.json",
    "ts-build:esnext": "tsc --project tsconfig.esnext.json",
    "ts-watch": "tsc --watch",
    "ci-test": "jest --coverage",
    "ci-build": "npm run ts-build:ci-cd && npm run ci-test",
    "ci-watch": "jest --coverage --watchAll"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "jsdom",
    "roots": [
      "targets/ci-cd/__tests__"
    ],
    "collectCoverageFrom": [
      "targets/ci-cd/iterator-cascade-callbacks.js",
      "targets/ci-cd/iterator-cascade-callbacks-asynchronously.js",
      "targets/ci-cd/lib/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules/"
    ],
    "testPathIgnorePatterns": [
      "targets/ci-cd/__tests__/lib"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      }
    },
    "coverageReporters": [
      "text"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/javascript-utilities/iterator-cascade-callbacks.git"
  },
  "keywords": [
    "generator",
    "iterator",
    "library",
    "module"
  ],
  "author": "S0AndS0",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/javascript-utilities/iterator-cascade-callbacks/issues"
  },
  "homepage": "https://github.com/javascript-utilities/iterator-cascade-callbacks#readme",
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "@types/node": "^20.5.9",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  }
}
