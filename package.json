{
  "name": "iterator-cascade-callbacks",
  "version": "0.0.6",
  "description": "JavaScript/TypeScript Iterables with Rust-like ergonomics: `.filter`, `.map`, and more!",
  "main": "./targets/mjs/index.mjs",
  "module": "./targets/mjs/index.mjs",
  "types": "./targets/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./targets/mjs/index.mjs",
      "require": "./targets/cjs/index.cjs",
      "types": "./targets/types/index.d.ts"
    }
  },
  "scripts": {
    "tsup:cjs:all": "npm run tsup:cjs:index && npm run tsup:cjs:synchronous && npm run tsup:cjs:asynchronous",
    "tsup:cjs:index": "tsup --config .config/tsup/tsup.cjs.config.index.ts",
    "tsup:cjs:asynchronous": "tsup --config .config/tsup/tsup.cjs.config.asynchronous.ts",
    "tsup:cjs:synchronous": "tsup --config .config/tsup/tsup.cjs.config.synchronous.ts",
    "tsup:mjs:all": "npm run tsup:mjs:index && npm run tsup:mjs:synchronous && npm run tsup:mjs:asynchronous",
    "tsup:mjs:index": "tsup --config .config/tsup/tsup.mjs.config.index.ts",
    "tsup:mjs:asynchronous": "tsup --config .config/tsup/tsup.mjs.config.asynchronous.ts",
    "tsup:mjs:synchronous": "tsup --config .config/tsup/tsup.mjs.config.synchronous.ts",
    "tsup:all": "npm run tsup:cjs:all && npm run tsup:mjs:all",
    "clean:all": "npm run clean:cjs ; npm run clean:mjs ; npm run clean:ci-cd",
    "clean:cjs": "rm -r targets/cjs",
    "clean:mjs": "rm -r targets/mjs",
    "clean:ci-cd": "rm -r targets/mjs",
    "ci:all": "npm run ci:lint && npm run ci:build && npm run ci:test",
    "ci:build": "tsc  --project tsconfig.json",
    "ci:lint": "eslint ts",
    "ci:test": "jest --coverage",
    "py-serve": "python3 -m http.server --bind 127.0.0.1 9080"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "jsdom",
    "roots": [
      "targets/ci-cd/__tests__"
    ],
    "collectCoverageFrom": [
      "targets/ci-cd/asynchronous/*.js",
      "targets/ci-cd/synchronous/*.js",
      "targets/ci-cd/lib/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules/"
    ],
    "testPathIgnorePatterns": [
      "targets/ci-cd/__tests__/lib"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      },
      "./*/lib/runtime-types*": {
        "statements": 0,
        "branches": 0,
        "functions": 0,
        "lines": 0
      }
    },
    "coverageReporters": [
      "text"
    ]
  },
  "eslintConfig": {
    "reportUnusedDisableDirectives": true,
    "ignorePatterns": [],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:@typescript-eslint/recommended-requiring-type-checking",
      "plugin:@typescript-eslint/strict",
      "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "tsconfigRootDir": ".",
      "project": [
        "./tsconfig.json"
      ]
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "root": true,
    "ignorePatterns": [
      "*/__tests__/**/*"
    ],
    "rules": {}
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/javascript-utilities/iterator-cascade-callbacks.git"
  },
  "keywords": [
    "generator",
    "iterator",
    "library",
    "module"
  ],
  "author": "S0AndS0",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/javascript-utilities/iterator-cascade-callbacks/issues"
  },
  "homepage": "https://github.com/javascript-utilities/iterator-cascade-callbacks#readme",
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "@types/node": "^20.5.9",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "ts-jest": "^29.1.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  }
}
